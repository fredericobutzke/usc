input goLM 0
input Rack 1
input REreq 0
input err1 0
input err0 0

input a 0
input b 0

output clk 0
output sample 0
output goML 0
output REack 0
output Rreq 0

0	  1	  goLM+  Rack-    | clk+ Rreq+
1	  2	  REreq+		      | sample+
2	  3	  err0+			      | clk- goML+
2	  4	  err1+			      | clk- goML+
3	  5	  a+      	      | sample- REack+
4	  7	  b+      	      | sample- REack+
5	  8	  a- err0- 	      | 
7	  8	  b- err1-        |
8	  9	  goLM-  Rack+    | clk+ Rreq-
9	  10	REreq-			    | sample+
10	11	err0+			      | clk- goML-
10	12	err1+			      | clk- goML-
11	13	a+      	      | sample- REack-
12	14	b+      	      | sample- REack-
13	0	  a- err0-        | 
14  0   b- err1-        |


clk =
  goLM' Rack err1' err0' REack +
  goLM Rack' err1' err0' REack' 

sample =
  REreq' a' b' REack +
  REreq a' b' REack' 

goML =
  sample' goML +
  err1 Rreq +
  err0 Rreq +
  err1' err0' REack 

REack =
  goML REack +
  a Rreq +
  b Rreq +
  a' b' REack 

Rreq =
  goLM Rack' +
  goLM Rreq +
  Rack' Rreq 
